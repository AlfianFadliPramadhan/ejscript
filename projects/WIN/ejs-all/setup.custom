  <PropertyGroup>
    <BuildDependsOn>
        Setup;
        $(BuildDependsOn);
    </BuildDependsOn>
  </PropertyGroup>

  <ItemGroup>
    <IncludeFiles Include="$(ProjectDir)buildConfig.default" />
    <ExportFiles  Include="$(ProjectDir)..\..\..\out\inc\buildConfig.h" />
  </ItemGroup>

  <Target Name="Setup" Inputs="@(IncludeFiles)" Outputs="@(ExportFiles)">
    <Copy SkipUnchangedFiles="true" SourceFiles="@(IncludeFiles)" DestinationFolder="$(ProjectDir)..\..\..\out\inc" />
  </Target>

