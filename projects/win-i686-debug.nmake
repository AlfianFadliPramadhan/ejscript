        $(CONFIG)\obj\manager.obj
	"$(LD)" -out:$(CONFIG)\bin\ejsman -entry:WinMainCRTStartup -subsystem:Windows $(LDFLAGS) $(LIBPATHS) $(CONFIG)\obj\manager.obj $(LIBS) libmpr.lib

