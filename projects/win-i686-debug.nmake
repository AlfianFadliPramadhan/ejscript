$(CONFIG)\inc\mpr.h: 
	-if exist win-i686-debug\inc\mpr.h del /Q win-i686-debug\inc\mpr.h
	copy /Y src\deps\mpr\mpr.h win-i686-debug\inc\mpr.h

$(CONFIG)\inc\mprSsl.h: 
	-if exist win-i686-debug\inc\mprSsl.h del /Q win-i686-debug\inc\mprSsl.h
	copy /Y src\deps\mpr\mprSsl.h win-i686-debug\inc\mprSsl.h

$(CONFIG)\obj\mprLib.obj: \
        src\deps\mpr\mprLib.c \
        $(CONFIG)\inc\buildConfig.h
	"$(CC)" -c -Fo$(CONFIG)\obj\mprLib.obj -Fd$(CONFIG)\obj $(CFLAGS) $(DFLAGS) -I$(CONFIG)\inc -I$(CONFIG)\inc src\deps\mpr\mprLib.c

$(CONFIG)\bin\libmpr.dll:  \
        $(CONFIG)\inc\mpr.h \
        $(CONFIG)\inc\mprSsl.h \
        $(CONFIG)\obj\mprLib.obj
	"$(LD)" -dll -out:$(CONFIG)\bin\libmpr.dll -entry:_DllMainCRTStartup@12 -def:$(CONFIG)\bin\libmpr.def $(LDFLAGS) $(LIBPATHS) $(CONFIG)\obj\mprLib.obj $(LIBS)

$(CONFIG)\obj\manager.obj: \
        src\deps\mpr\manager.c \
        $(CONFIG)\inc\buildConfig.h
	"$(CC)" -c -Fo$(CONFIG)\obj\manager.obj -Fd$(CONFIG)\obj $(CFLAGS) $(DFLAGS) -I$(CONFIG)\inc -I$(CONFIG)\inc src\deps\mpr\manager.c

$(CONFIG)\bin\ejsman:  \
