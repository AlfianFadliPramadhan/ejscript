PKG_DIR="$1"
INS_DIR="$2"
NAME="!!BLD_PREFIX!!"
BIN="!!ORIG_BLD_BIN_PREFIX!!"
path="${INS_DIR}${NAME}"

${BIN}/linkup Install
exit 0
